                                <button type="submit" class="btn btn-danger btn-sm font-sm rounded d-flex justify-content-center align-items-center" >
                                    <i class="material-icons md-delete_forever"></i>Delete
                                </button>
                            </form>
                            </div>
                        </div> <!-- row .// -->
                    </article> <!-- itemlist  .// -->
                     <%})}%>
