<%- include('../layout/headerBack.ejs')%>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Users List</h2>
                    <p>All users.</p>
                </div>
                
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row align-items-center">
                        <div class="col col-check flex-grow-0">
                            <div class="form-check ms-2">
                                <input class="form-check-input" type="checkbox" value="">
                            </div>
                        </div>
                        <div class="col-md-3 col-12 me-auto mb-md-0 mb-3">
                            <select class="form-select">
                                <option selected>status</option>
                                <option>Electronics</option>
                                <option>Clothes</option>
                                <option>Automobile</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <% if(users !== 'undefined'){
                        users.forEach(user=>{%>
                    <article class="itemlist">
                        <div class="row align-items-center">
                            <div class="col col-check flex-grow-0">
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox">
                                </div>
                            </div>
                            <div class="col-lg-3 col-sm-3 col-6 col-name">
                                <a class="itemside" href="#">
                                    <div class="info">
                                        <h6 class="mb-0"><%= user.name%></h6>
                                    </div>
                                </a>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-4 col-price"> <span><%= user.email%></span> </div>
                            <div class="col-lg-3 col-sm-3 col-4 col-status">
                                <span><%= user.mobile%></span>
                            </div>
                            <% if (user.status === true){%>
                                <div class="col-lg-1 col-sm-2 col-2 col-status">
                                    <span class="btn btn-sm rounded-pill alert-success" onclick="toggleUserStatus('<%= user._id%>')">Active</span>
                                </div>
                                <%}else{%>
                                    <div class="col-lg-1 col-sm-2 col-2 col-status">
                                        <span class="btn btn-sm rounded-pill alert-danger" onclick="toggleUserStatus('<%= user._id%>')">Disabled</span>
                                    </div>
                                <%}%>
                        </div> <!-- row .// -->
                    </article> <!-- itemlist  .// -->
                    <%})}%>
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
            <div class="pagination-area mt-30 mb-50">
                <nav aria-label="Page navigation example">
                    <ul class="pagination justify-content-start">
                        <li class="page-item active"><a class="page-link" href="#">01</a></li>
                        <li class="page-item"><a class="page-link" href="#">02</a></li>
                        <li class="page-item"><a class="page-link" href="#">03</a></li>
                        <li class="page-item"><a class="page-link dot" href="#">...</a></li>
                        <li class="page-item"><a class="page-link" href="#">16</a></li>
                        <li class="page-item"><a class="page-link" href="#"><i class="material-icons md-chevron_right"></i></a></li>
                    </ul>
                </nav>
            </div>
        </section> <!-- content-main end// -->
        <%- include('../layout/footerBack.ejs')%>
