                                    <div class="info">
                                        <h6 class="mb-0"><%= category.name%></h6>
                                    </div>
                                </a>
                            </div>
                            <% if (category.list === true){%>
                            <div class="col-lg-2 col-sm-2 col-2 col-status">
                                <span class="badge rounded-pill alert-success">Active</span>
                            </div>
                            <%}else{%>
                                <div class="col-lg-2 col-sm-2 col-2 col-status">
                                    <span class="badge rounded-pill alert-danger">Disabled</span>
                                </div>
                            <%}%>
                            
                            <div class="col-lg-4 col-sm-4 col-4 col-action d-flex justify-content-evenly">
                                <a href="/admin/category-edit/<%= category._id%>" class="btn btn-sm font-sm rounded btn-brand d-flex justify-content-center align-items-center">
                                    <i class="material-icons md-edit"></i> Edit
                                </a>
                            <form action="/admin/category-delete/<%= category._id %>?_method=DELETE" method="POST" onsubmit="return confirm('Are you sure you want to delete <%= category.name %> ?');">
